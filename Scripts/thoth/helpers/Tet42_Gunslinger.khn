local __util = require 'larian.util'

function IsGSSmite()
    return SpellId('GS_FTH_MH') | SpellId('GS_FTH_OH') | SpellId('Projectile_GS_Banishing_Shot')
end

function WeenieCheck()
    return HasUseCosts('GSAmmo') | HasUseCosts('GSAmmoOffhand') | HasUseCosts('GB_Ammo_Mainhand') | HasUseCosts('GB_Ammo_Offhand')
end
